{
  "root": true,
  "env": {
    "node": true
  },
  "globals": {
    "document": "readonly",
    "localStorage": "readonly"
  },
  "parserOptions": {
    "sourceType": "script"
  },
  "extends": [
    "eslint-config-airbnb-base",
    "prettier"
  ],
  "plugins": ["eslint-plugin-prettier", "prettier"],
  "rules": {
    "prettier/prettier": "error"
  }
}
